{
  "name": "testweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.0",
    "eslint-plugin-vue": "^5.2.1",
    "vue-template-compiler": "^2.6.6",
    "vuetify": "^1.5.1",
    "vuetify-loader": "^1.2.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "axios": "^0.18.0",
    "sass-loader": "^7.1.0",
    "node-sass": "^4.11.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
